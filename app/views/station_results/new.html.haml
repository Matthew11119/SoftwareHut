%h1 New station_result

= render 'form'


%table
  %tr
    %th name
    %th Criteria discription
    %th Criteria Critical
    %th Station id

  - @osces.each do |osce|
    %tr
      %td= osce.id
      %td= osce.criteria_description
      %td= osce.criteria_critical
      %td= osce.station_id

%p= link_to 'Back', station_results_path
